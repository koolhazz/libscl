#!/bin/sh


# ******************************************************************************
#
# ******************************************************************************

echo "Removing executable test files."


# ******************************************************************************
#
# ******************************************************************************

for d in t*; do
	[ ! -d ${d} ] && continue
	for f in ${d}[0-9]; do
		[ ! -f ${f} ] && continue
		echo "${f}"
		rm ${f}
	done
done


# ******************************************************************************
#
# ******************************************************************************

unset -v d
unset -v f


# ******************************************************************************
#
# ******************************************************************************

exit 0
